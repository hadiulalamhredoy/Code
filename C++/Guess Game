#include<iostream>
using namespace std;
int main()
{

int guessNumber , randomNumber;
cin >> guessNumber;

randomNumber = 1+ rand()%5;

if(guessNumber==randomNumber)
{
cout << "You have won" <<endl<<endl;

}
else {

cout<<"You have lost.try again "<<endl;
cout <<"Random Number was : "<<randomNumber<<endl<<endl;
}

}
